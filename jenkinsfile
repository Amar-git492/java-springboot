pipeline {
    agent any 
    stages {
        stage('build') {
            steps {
                echo 'build'
            }
        }
        stage('test') {
            steps {
                echo 'test'
                sh 'mvn clean package'
            }
        }
        stage('UAT') {
            steps {
                echo 'UAT'
                sh 'mvn test'
            }
        }
        stage('Stagging') {
            steps {
                echo 'Stagging'
            }
        }
        stage('production') {
            steps {
                echo 'production'
            }
        }
    }
    post { 
        always { 
            echo 'always'
        }
        failure { 
            echo 'failure'
        }
        success { 
            echo 'success'
        }
    }
}